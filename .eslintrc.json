{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "react-app",
        "react-app/jest",
        "prettier"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "@typescript-eslint",
        "prettier"
    ],
    "rules": {
        "prettier/prettier":[
            "warn",{
                "tabWidth": 4,
                "semi": true,
                "singleQuote": true,
                "trailingComma": "none",
                "printWidth": 120,
                "useTabs": true,
                "endOfLine":"auto"
            }
        ]
    }
}
